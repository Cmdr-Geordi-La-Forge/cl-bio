<!-- Variation instance -->
<!ELEMENT Variation-inst (
        Variation-inst_type, 
        Variation-inst_delta)>

<!ELEMENT Variation-inst_type (%INTEGER;)>

<!--
    identity	-  delta = this
    inv	-  inversion: delta =
         reverse-comp(feat.location)
    snp	-  delins where len(del) = len(ins) = 1
    mnp	-  delins where len(del) = len(ins) > 1
    delins	-  delins where len(del) != len(ins)
    del	-  deltaseq is empty
    ins	-  deltaseq contains [this, ins] or [ins, this]
    microsatellite	-  location describes tandem sequence;
           delta is the repeat-unit with a multiplier
    transposon	-  delta refers to equivalent sequence in
         another location.
           ext-loc describes donor sequence, if known
         (could be location itself)
    cnv	-  general CNV class, indicating "local"
         rearrangement
    direct-copy	-  Below are four possible copy configurations,
         where delta is a seq-loc on the same sequence.
         If the repeat is represented on the sequence, it is
         described like a transposon; if it is a de-novo
         repeat, it is described like an insertion.
         delta sequence is located upstream, same
         strand
    rev-direct-copy	-  delta sequence is downstream, same strand
    inverted-copy	-  delta sequence is upstream, opposite strand
    everted-copy	-  delta sequence is downstream, opposite strand
    translocation	-  feat.location is swapped with delta (i.e.
         reciprocal transposon)
-->
<!ATTLIST Variation-inst_type value (
        unknown |
        identity |
        inv |
        snp |
        mnp |
        delins |
        del |
        ins |
        microsatellite |
        transposon |
        cnv |
        direct-copy |
        rev-direct-copy |
        inverted-copy |
        everted-copy |
        translocation |
        prot-missense |
        prot-nonsense |
        prot-neutral |
        prot-silent |
        prot-other |
        other
        ) #IMPLIED >


